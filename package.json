{
  
  "name": "react-base",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --ignore-path=.prettierignore",
      "git add"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "scripts": {

    
    "start": "env-cmd -f .env.dev react-app-rewired start",
    "build:dev": "env-cmd -f .env.dev react-app-rewired build",
    "build:test": "env-cmd -f .env.test react-app-rewired build",
    "build:production": "env-cmd -f .env.production react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint --ignore-path=.prettierignore",
    "lint:fix": "eslint --fix --ignore-path=.prettierignore",
    "prettify": "prettier --write **/*.{ts,tsx,js,jsx,css,scss,json}"
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.3.7",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "prettier": "^2.6.1",
    "react-app-rewired": "^2.2.1",
    "redux-logger": "^3.0.6",
    "sass-loader": "^12.6.0"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@reduxjs/toolkit": "^1.8.3",
    "ahooks": "^3.3.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "cra-template": "1.1.3",
    "env-cmd": "^10.1.0",
    "husky": "4",
    "i18next": "^21.6.14",
    "i18next-xhr-backend": "^3.2.2",
    "localforage": "^1.10.0",
    "node-sass": "^7.0.1",
    "react": "^18.0.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^18.0.0",
    "react-i18next": "^11.16.2",
    "react-icons": "^4.4.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "scss-reset": "^1.2.2",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
